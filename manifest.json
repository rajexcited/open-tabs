{
  "manifest_version": 2,
  "name": "Open Tabs",
  "version": "0.0.1",
  "description": "Displays the count and list of open tabs from current or all windows (configurable) and allows you to select and/or close them. Also shows history of open tabs.",
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_name": "Open Tabs",
    "default_icon": "images/icon16-min.png",
    "default_title": "Open Tabs"
  },
  "icons": {
    "16": "images/icon16-min.png",
    "48": "images/icon48-min.png",
    "128": "images/icon128-min.png"
  },
  "permissions": [
    "<all_urls>"
  ],
  "author": "Raj Sheth"
}
